{
  "name": "nestjs-prisma-zod-swagger",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "update": "ncu -u -x typeorm",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "dotenv -e .env.development -- nest start",
    "start:dev": "run-s prebuild && dotenv -e .env.development -- nest start --watch",
    "start:debug": "run-s prebuild && dotenv -e .env.development -- nest start --debug --watch",
    "start:prod": "dotenv -e .env.production -- node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prisma:dev": "run-s prisma:prismixer prisma:generate:dev prisma:migrate:dev",
    "prisma:test": "run-s prisma:prismixer prisma:generate:test prisma:migrate:test",
    "prisma:studio": "prisma studio",
    "prisma:cleanup": "dotenv -e .env.test -- prisma migrate reset --force",
    "prisma:prismixer": "prismixer",
    "prisma:migrate:dev": "dotenv -e .env.development -- prisma migrate dev --name init",
    "prisma:migrate:test": "dotenv -e .env.test -- prisma migrate dev --name test",
    "prisma:generate:dev": "dotenv -e .env.development -- prisma generate",
    "prisma:generate:test": "dotenv -e .env.test -- prisma generate",
    "test": "dotenv -e .env.test -- jest",
    "test:watch": "dotenv -e .env.test -- jest --watch",
    "test:cov": "dotenv -e .env.test -- jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "dotenv -e .env.test -- jest --config ./jest-e2e.config.js"
  },
  "dependencies": {
    "@anatine/zod-mock": "^3.5.6",
    "@anatine/zod-nestjs": "^1.7.3",
    "@anatine/zod-openapi": "^1.9.6",
    "@fastify/static": "^6.5.0",
    "@fastify/swagger": "^7.4.1",
    "@nestjs/common": "^9.0.7",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.7",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-fastify": "^9.0.7",
    "@nestjs/swagger": "^6.0.4",
    "@nestjs/typeorm": "^9.0.0",
    "@prisma/client": "^4.1.1",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "mysql2": "^2.3.3",
    "openapi3-ts": "^2.0.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "typeorm": "^0.3.7",
    "zod": "^3.17.10"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.3.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.7",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "28.1.6",
    "@types/node": "^18.6.3",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prisma": "^4.1.1",
    "prismixer": "^1.0.4",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  }
}

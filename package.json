{
  "name": "nestjs-openapi-prisma",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "update": "ncu -u -x typeorm",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "dotenv -e .env.development -- nest start",
    "start:dev": "run-s prebuild && dotenv -e .env.development -- nest start --watch",
    "start:debug": "run-s prebuild && dotenv -e .env.development -- nest start --debug --watch",
    "start:prod": "dotenv -e .env.production -- node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "dotenv -e .env.test -- jest",
    "test:watch": "dotenv -e .env.test -- jest --watch",
    "test:cov": "dotenv -e .env.test -- jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "dotenv -e .env.test -- jest --config ./jest-e2e.config.js",
    "typeorm:dev": "dotenv -e .env.development -- run-s typeorm:create typeorm:run",
    "typeorm:test": "dotenv -e .env.test -- run-s typeorm:create typeorm:run",
    "typeorm:create": "typeorm migration:create -n dev",
    "typeorm:run": "typeorm migration:run",
    "typeorm:revert": "typeorm migration:revert",
    "typeorm:clear": "typeorm cache:clear"
  },
  "dependencies": {
    "@anatine/zod-mock": "^3.1.1",
    "@anatine/zod-nestjs": "^1.3.1",
    "@anatine/zod-openapi": "^1.3.0",
    "@faker-js/faker": "^6.3.1",
    "@nestjs/common": "^8.4.5",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.4.5",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-fastify": "^8.4.5",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/typeorm": "^8.0.3",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "fastify-swagger": "^5.2.0",
    "mysql2": "^2.3.3",
    "openapi3-ts": "^2.0.2",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "typeorm": "^0.2.45",
    "zod": "^3.16.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.5",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.5",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "27.5.1",
    "@types/node": "^17.0.33",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.2",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.4"
  }
}
